set initmode = i
set easyclick_bgcolor=FFFFFF
set easyclick_fontcolor=000000
set easyclick_colordecay=1000
set easyclick_fontname=Arial
set easyclick_fontsize=14
set easyclick_fontweight=1000

inoremap <f23> <easyclick_all><click_left><space>
inoremap <f24> <easyclick_all><click_right><space>
inoremap <ctrl-[> <hotkey_copy>
inoremap <ctrl-]> <hotkey_copy>
set hintkeys = arstneio
