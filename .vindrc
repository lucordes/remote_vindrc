set initmode = i
set easyclick_bgcolor=FFFFFF
set easyclick_fontcolor=000000
set easyclick_colordecay=150
set easyclick_fontname=Consolas
set easyclick_fontsize=16
set easyclick_fontweight=100

imap <Win-e> <to_command>execute C:\Program Files\Double Commander\doublecmd.exe<cr>
inoremap <lalt-lctrl-a> <easy_click_left>
inoremap <lalt-lctrl-o> <easy_click_right>
inoremap <ctrl-[> <hotkey_copy>
inoremap <ctrl-]> <hotkey_copy>
set hintkeys = arstneio
